{
  "name": "demo-blog-backend",
  "description": "Demo Blog BackEnd - API supplier and Swagger Docs",
  "version": "3.0.0",
  "private": false,
  "license": "MIT",
  "readme": "README.md",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "nodemon --config=./nodemon.json",
    "start-production-pm2": "pm2 start ./pm2-production.config.js --no-daemon",
    "start-production": "node -r ./tsconfig-paths-bootstrap.js ./build/index.js",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register",
    "test-report": "nyc npm run test -- --reporter mochawesome --reporter-options reportDir='./build/test-results/',reportFilename='index',reportPageTitle='demo-blog-test mocha test results',inline=true,cdn=true,charts=true,autoOpen=false",
    "build": "rimraf ./build && tsc --build tsconfig.json && npm run test-report",
    "lint": "eslint ./**/*.{js,jsx,ts,tsx}",
    "prepare": "husky install"
  },
  "dependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "ajv": "8.6.3",
    "ajv-formats": "2.1.1",
    "convert-json-schema-to-mongoose": "0.3.2",
    "dotenv": "10.0.0",
    "fastify": "3.22.1",
    "fastify-cors": "6.0.2",
    "fastify-env": "2.1.0",
    "fastify-helmet": "5.3.2",
    "fastify-plugin": "3.0.0",
    "fastify-swagger": "4.12.6",
    "immer": "9.0.7",
    "json-schema-to-ts": "1.6.4",
    "json5": "2.2.0",
    "mongoose": "6.0.12",
    "mongoose-paginate-v2": "1.4.2",
    "pm2": "5.1.2",
    "rimraf": "3.0.2",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/chai": "4.2.22",
    "@types/mocha": "9.0.0",
    "@types/mongoose": "5.11.97",
    "@types/mongoose-paginate-v2": "1.4.0",
    "@types/node": "16.11.6",
    "@types/uuid": "8.3.3",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "chai": "4.3.4",
    "chai-string": "1.5.0",
    "eslint": "8.1.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-jsdoc": "37.0.3",
    "husky": "7.0.4",
    "mocha": "9.1.3",
    "mochawesome": "6.3.1",
    "nodemon": "2.0.14",
    "nyc": "15.1.0",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "ts-node": "10.4.0"
  }
}
